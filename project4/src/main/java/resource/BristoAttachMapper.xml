<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="project4.mapper.BristoAttachMapper">


	<insert id="insert"
		parameterType="project4.domain.BristoAttachVO">
		INSERT INTO bristo_attach
		values(
		#{uuid},
		#{uploadPath},
		#{fileName},
		#{filetype},
		#{bristokey}
		)
	</insert>


	<delete id="delete" parameterType="int">
		delete
		from bristo_attach
		where
		bristokey=#{bristokey}
	</delete>

	<select id="findByBno"
		resultType="project4.domain.BristoAttachVO">
		select * from bristo_attach where bristokey=#{bristokey}
	</select>
</mapper>